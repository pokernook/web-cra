{
  "name": "web",
  "description": "Web App built with Next.js",
  "repository": "github:pokernook/web",
  "version": "0.0.0",
  "scripts": {
    "generate": "graphql-codegen -r dotenv/config",
    "dev": "concurrently --kill-others \"npm run generate -- --watch\" \"next dev\"",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "postinstall": "npm run generate",
    "prebuild": "npm run generate",
    "build": "next build",
    "start": "next start",
    "sb": "start-storybook",
    "sb:build": "build-storybook"
  },
  "dependencies": {
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "next": "^10.0.1",
    "next-urql": "^2.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.11.3",
    "react-is": "^17.0.1",
    "theme-ui": "^0.3.1",
    "urql": "^1.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@graphql-codegen/typescript-urql": "^2.0.3",
    "@storybook/addon-actions": "^6.1.0",
    "@storybook/addon-essentials": "^6.1.0",
    "@storybook/addon-links": "^6.1.0",
    "@storybook/addons": "^6.1.0",
    "@storybook/react": "^6.1.0",
    "@storybook/theming": "^6.1.0",
    "@types/node": "^14.11.10",
    "@types/react": "^17.0.0",
    "@types/theme-ui": "^0.3.7",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "node-fetch": ">=2.6.1",
    "prettier": "^2.1.2",
    "typescript": "^4.1.0"
  }
