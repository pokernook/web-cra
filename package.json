{
  "name": "web",
  "description": "Web App built with React",
  "repository": "github:pokernook/web",
  "scripts": {
    "generate": "graphql-codegen -r dotenv/config",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "dev:react": "react-scripts start",
    "dev:codegen": "npm run generate -- --watch",
    "dev": "concurrently --kill-others npm:dev:*",
    "build": "react-scripts build",
    "start": "serve --single build"
  },
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-urql": "^2.0.3",
    "@theme-ui/color": "^0.6.0-alpha.4",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/seedrandom": "^2.4.28",
    "@urql/exchange-graphcache": "^4.0.0",
    "concurrently": "^5.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "framer-motion": "^3.2.2-rc.1",
    "graphql": "^15.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.1",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "seedrandom": "^3.0.5",
    "serve": "^11.3.2",
    "theme-ui": "^0.6.0-alpha.4",
    "typescript": "^4.1.3",
    "urql": "^1.11.6",
    "web-vitals": "^1.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
